  },
  "description": "A tool platform for Nokia oam.",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "e2e": "ng e2e",
    "build": "ng build --base-href ./ && nx build electron",
    "build:main": "nx build electron",
    "dev": "nx e2e ng-client-e2e --watch",
    "ut": "nx run-many --all --target=test",
    "lint": "nx workspace-lint && ng lint",
    "precommit": "npm run lint && npm run ut && npm run e2e",
    "postinstall": "patch-package && exitzero electron-builder install-app-deps",
    "package": "yarn run build --prod && nx run electron:package --platform windows --arch x64 --publishPolicy never",
    "publish": "./scripts/publish.sh",
    "publish-only": "nx run electron:make --platform windows --arch x64 --publishPolicy always"
  },
  "private": true,
  "lint-staged": {
    "*": "yarn lint"
  },
  "dependencies": {
